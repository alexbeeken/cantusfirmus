<h3 class="text-left"> Current Examples </h3>

<div class="row">
  <div class="col-md-12">
    <ol>
      <% @list.each do |example| %>
        <li>
          <%= link_to "#{example.showable_notes}", example_path(example.id), :method =>"Get", :tonic => "#{example.tonic}" %>
        </li>
      <% end %>
    </ol>

      <div class="container">

        <div class="col-md-4">
          <%= link_to "Generate New Example", new_example_path, :method =>"Get", :class => "btn btn-info" %>
        </div>

        <div class="col-md-4">
          <%= link_to "Reset All Examples", reset_path, :method =>"Get", :class => "btn btn-warning" %>
        </div>
      </div>
    </div>
  </div>
</div>

<br>
